{
    "name": "Wallpapers",
    "version": "v1.0",
    "author": "joamjoamjoam",
    "target": "System-Wide",
    "description": "Sets Wallpapers. What did you Expect?",
    "manifest_version": 2,
    "inject": {
        "wallpaperCSS/homeScreen.css": [
            "SP"
        ],
        "wallpaperCSS/keyboard.css": [
            "SP"
        ],
        "wallpaperCSS/library.css": [
            "SP"
        ],
        "wallpaperCSS/lockScreen.css": [
            "SP"
        ]
    },
    "patches": {
        "Disable Home Overlay": {
            "default": "Yes",
            "type": "checkbox",
            "values": {
                "No": {},
                "Yes": {
                    "homeScreenOverlay/disabled.css": [
                        "SP"
                    ]
                }
            }
        },
        "Set Background Color": {
            "default": "Blue",
            "type": "dropdown",
            "components": [
                {
                    "name": "Custom Color",
                    "type": "color-picker",
                    "default": "blue",
                    "css_variable": "myBackgroundColor",
                    "tabs": [
                        "SP",
                        "QuickAccess",
                        "MainMenu"
                    ],
                    "on": "Custom"
                }
            ],
            "values": {
                "Blue": {},
                "Custom": {}
            }
        },
        "Home Screen Image": {
            "default": "None",
            "type": "dropdown",
            "values": {
                "None": {
                    "generatedCSSFiles/fallbackCSS/WPRHomeScreenWallPaper.css": [
                        "SP"
                    ]
                },
                "62Fb2B49E5B01": {
                    "generatedCSSFiles/sharedB64Images/62Fb2B49E5B01.css": [
                        "SP"
                    ],
                    "generatedCSSFiles/WPRHomeScreenWallPaper/62Fb2B49E5B01.css": [
                        "SP"
                    ]
                },
                "Lockscreen": {
                    "generatedCSSFiles/sharedB64Images/Lockscreen.css": [
                        "SP"
                    ],
                    "generatedCSSFiles/WPRHomeScreenWallPaper/Lockscreen.css": [
                        "SP"
                    ]
                },
                "RotatingEarthLarge": {
                    "generatedCSSFiles/sharedB64Images/RotatingEarthLarge.css": [
                        "SP"
                    ],
                    "generatedCSSFiles/WPRHomeScreenWallPaper/RotatingEarthLarge.css": [
                        "SP"
                    ]
                }
            }
        },
        "Lock Screen Image": {
            "default": "None",
            "type": "dropdown",
