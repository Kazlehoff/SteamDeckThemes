{
    "name": "Icon Packer",
    "version": "v1.0",
    "author": "joamjoamjoam",
    "target": "System-Wide",
    "description": "Set Icons Sytmen Wide using Icon Packs.",
    "manifest_version": 2,
    "inject": {
        "ICPShared.css": [
          "SP", "QuickAccess", "MainMenu"
        ]
    },
    "patches": {
      "Lock Screen Type": {
        "default": "Xbox",
        "type": "dropdown",
        "values":{
            "Xbox": {
              "Icon Sets/Xbox Icons/Icons.css": [
                "SP", "QuickAccess", "MainMenu"
              ]
            }
        }
      }
    }
}